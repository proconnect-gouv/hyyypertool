{
  "name": "@~/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*/index.ts"
  },
  "exports": {
    "./*": "./src/*/index.ts"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf node_modules/.cache/tsc",
    "dev:api": "bun x run-z dev:api:clean dev:api:tsc dev:api:cp dev:api:format",
    "dev:api:clean": "bun run --filter @~/app.urls clean",
    "dev:api:cp": "cp ./node_modules/.cache/tsc/src/routes/index.d.ts '../app/urls/src/pattern.d.ts'",
    "dev:api:format": "bun run --filter @~/app.urls format",
    "dev:api:tsc": "bun run build"
  },
  "dependencies": {
    "@~/app.core": "workspace:*",
    "@~/app.layout": "workspace:*",
    "@~/app.middleware": "workspace:*",
    "@~/app.sentry": "workspace:*",
    "@~/app.ui": "workspace:*",
    "@~/app.urls": "workspace:*",
    "@~/auth.api": "workspace:*",
    "@~/moderations.api": "workspace:*",
    "@~/organizations.api": "workspace:*",
    "@~/users.api": "workspace:*",
    "@~/zammad.api": "workspace:*",
    "@~/zammad.lib": "workspace:*",
    "@hono/node-server": "1.19.5",
    "@hono/sentry": "1.2.2",
    "await-to-js": "3.0.0",
    "consola": "3.4.2",
    "hono": "4.9.8",
    "ts-pattern": "5.8.0",
    "youch": "3.3.4"
  },
  "devDependencies": {
    "@~/config.typescript": "workspace:*"
  }
}

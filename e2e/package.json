{
  "name": "e2e",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cd .. && bun run build && bun run db:migrate",
    "migrate": "cd .. && bun db:migrate",
    "open": "cypress open -b electron --e2e",
    "seed": "cd .. && bun db:seed",
    "start": "cd .. && bun start",
    "studio": "cypress open -b electron --e2e",
    "test": "cypress run -e filterSpecs=true"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.8",
    "@proconnect-gouv/proconnect.identite.database": "1.2.0",
    "@testing-library/cypress": "10.1.0",
    "cypress": "15.10.0",
    "cypress-wait-until": "3.0.2",
    "typescript": "5.9.3"
  },
  "trustedDependencies": [
    "cypress"
  ]
}
